{
  "name": "moyu-lab-node",
  "version": "1.0.0",
  "description": "墨鱼仔的触手实验室-node端",
  "private": true,
  "egg": {
    "typescript": true,
    "declarations": true
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=moyu-lab-node",
    "stop": "egg-scripts stop --title=moyu-lab-node",
    "dev": "egg-bin dev",
    "debug": "egg-bin debug -r egg-ts-helper/register",
    "test-local": "egg-bin test",
    "test": "npm run lint -- --fix && npm run test-local",
    "cov": "egg-bin cov",
    "tsc": "ets && tsc -p tsconfig.json",
    "ci": "npm run lint && npm run cov && npm run tsc",
    "autod": "autod",
    "lint": "tslint --project . -c tslint.json",
    "fixlint": "tslint --fix --project . -c tslint.json",
    "clean": "ets clean",
    "prod": "npm run tsc && export NODE_ENV=production && EGG_SERVER_ENV=prod egg-scripts start --daemon --sticky --title=moyu-lab-node"
  },
  "dependencies": {
    "egg": "^2.22.2",
    "egg-scripts": "^2.11.0"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.0.8",
    "autod": "^3.1.0",
    "autod-egg": "^1.1.0",
    "egg-bin": "^5.9.0",
    "egg-ci": "^1.12.0",
    "egg-cors": "^2.2.0",
    "egg-mock": "^3.23.1",
    "egg-mysql": "^3.0.0",
    "egg-oss": "^2.0.0",
    "egg-redis": "^2.4.0",
    "egg-session-redis": "^2.1.0",
    "egg-ts-helper": "^1.25.4",
    "egg-validate": "^2.0.2",
    "stream-wormhole": "^1.1.0",
    "tslib": "^1.10.0",
    "tslint": "^5.17.0",
    "tslint-config-egg": "^1.0.0",
    "typescript": "^3.5.2"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "ci": {
    "version": "8"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "eslintIgnore": [
    "coverage"
  ],
  "author": "moyu",
  "license": "MIT"
}